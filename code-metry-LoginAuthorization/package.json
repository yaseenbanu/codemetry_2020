{
	"name": "code-metry",
	"displayName": "code-metry",
	"description": "An Exclusive extension for those who are new to the programming, providing them help where and when required",
	"version": "5.0.0",
	"publisher": "code-metry-msit",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-metry.menu",
				"title": "Launch Metry"
			},
			{
				"command": "Create-Pull-Request",
				"title": "Code Metry : Submit Assignment"
			},
			{
				"command": "codemetry.displayTree",
				"title": "Menu"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "code-metry",
					"title": "Code Metry",
					"icon": "./resources/coding-graduate-icon.svg"
				}
			]
		},
		"views": {
			"code-metry": [
				{
					"id": "cm-menu-tree",
					"name": "Menu"
				},
				{
					"id": "cm-submissions-tree",
					"name": "Submission"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:uninstall": "node ./out/lifecycle.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/numeral": "0.0.28",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"moment-timezone": "^0.5.31",
		"node-cache": "^5.1.1",
		"node-fetch": "^2.6.1",
		"open": "^7.0.4",
		"ws": "^7.3.1"
	}
}
